#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules/vercel/dist/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules/vercel/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules/vercel/dist/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules/vercel/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/vercel@44.5.5_rollup@4.45.1/node_modules:/Users/gontsemaepa/Desktop/taskManage/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/vc.js" "$@"
else
  exec node  "$basedir/../../dist/vc.js" "$@"
fi
